<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:color="@color/color_tangerine_2" android:width="@dimen/dimen_common_1"/>
            <corners android:radius="@dimen/dimen_common_10"/>
            <gradient android:startColor="@color/color_black_4" android:endColor="@color/color_black_4"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:color="@color/color_tangerine_2" android:width="@dimen/dimen_common_1"/>
            <corners android:radius="@dimen/dimen_common_10"/>
            <gradient android:startColor="@color/color_white" android:endColor="@color/color_white"/>
        </shape>
    </item>
</selector>
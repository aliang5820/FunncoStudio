<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <include
        android:id="@id/llayout_head"
        layout="@layout/layout_activity_header_common"/>
    <include
        android:id="@id/llayout_foot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        layout="@layout/layout_bottom_save"/>
    <FrameLayout
        android:id="@id/layout_container"
        style="@style/style_layout_width_height_mm"
        android:layout_below="@id/llayout_head"
        android:layout_above="@id/llayout_foot">
    </FrameLayout>
    <TextView
        android:id="@id/id_notify"
        style="@style/style_layout_width_height_ww"
        android:text="@string/str_team_nullnotify"
        android:layout_centerInParent="true"
        android:visibility="gone"/>
</RelativeLayout>
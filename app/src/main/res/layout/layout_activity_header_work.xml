<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="@dimen/dimen_header_height"
    android:background="@drawable/shape_tv_begin"
    android:gravity="center_vertical"
    android:paddingLeft="10dp"
    android:paddingRight="20dp">
    <TextView
        android:id="@+id/tv_work_headr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:text=""
        android:clickable="true"
        android:background="@drawable/selector_titlebar_add">
    </TextView>
    <TextView
        android:id="@+id/tv_work_headm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:gravity="center"
        android:text="@string/upload"
        style="@style/style_base_white_18"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true">
    </TextView>
    <TextView
        android:id="@+id/tv_work_headl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        style="@style/style_base_white_18"
        android:gravity="center"
        android:padding="10dp"
        android:text="@string/edit"
        android:clickable="true">
    </TextView>
</RelativeLayout>
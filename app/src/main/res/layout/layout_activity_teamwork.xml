<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <include
        android:id="@id/llayout_head"
        layout="@layout/layout_activity_header"/>
    <com.funnco.funnco.view.listview.XListView
        android:id="@id/id_listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:animationCache="false"
        android:dividerHeight="0dp"
        android:layout_below="@id/llayout_head"
        android:layout_marginBottom="@dimen/dimen_common_15">
    </com.funnco.funnco.view.listview.XListView>
    <ImageButton
        android:id="@id/id_imagebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@color/color_translucent"
        android:src="@mipmap/common_team_work_edit"
        android:padding="@dimen/dimen_common_15"/>
</RelativeLayout>